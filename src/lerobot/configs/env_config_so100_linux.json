{
  "env": {
    "name": "real_robot",
    "fps": 30,
    "processor": {
      "control_mode": "gamepad",
      "observation": {
        "display_cameras": false,
        "add_joint_velocity_to_observation": true,
        "add_current_to_observation": true
      },
      "image_preprocessing": {
        "crop_params_dict": {},
        "resize_size": [128, 128]
      },
      "gripper": {
        "use_gripper": true,
        "gripper_penalty": 0.0
      },
      "reward_classifier": {
        "pretrained_path": "thewisp/hil_serl_move_cube_reward_classifier",
        "success_threshold": 0.7,
        "success_reward": 1.0
      },
      "reset": {
        "reset_time_s": 5.0,
        "control_time_s": 50.0,
        "fixed_reset_joint_positions": [
          0.0,
          0.0,
          0.0,
          90.0,
          0.0,
          30.0
        ],
        "terminate_on_success": true
      },
      "inverse_kinematics": {
        "urdf_path": "/home/feit/Documents/SO-ARM100/Simulation/SO101/so101_new_calib.urdf",
        "target_frame_name": "gripper_frame_link",
        "end_effector_bounds": {
            "min": [
                0.16,
                -0.19,
                0.005
            ],
            "max": [
                0.32,
                0.19,
                0.13
            ]
        },
        "end_effector_step_sizes": {
            "x": 0.005,
            "y": 0.005,
            "z": 0.005
        }
      },
      "max_gripper_pos": 30
    },
    "robot": {
        "type": "so100_follower",
        "port": "/dev/ttyACM1",
        "id": "black",
        "cameras": {
            "top": {
                "type": "opencv",
                "index_or_path": 2,
                "height": 480,
                "width": 640,
                "fps": 30
            },
            "wrist": {
                "type": "opencv",
                "index_or_path": 0,
                "height": 480,
                "width": 640,
                "fps": 30
            }
        }
    },
    "teleop": {
        "type": "gamepad",
        "use_gripper": true
    }
  },
  "dataset": {
    "repo_id": "thewisp/hil_serl_move_cube",
    "root": null,
    "task": "move_cube",
    "num_episodes_to_record": 15,
    "replay_episode": 0,
    "push_to_hub": true
  },
  "mode": null,
  "device": "cpu"
}